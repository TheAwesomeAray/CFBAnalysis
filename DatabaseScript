CREATE DATABASE [CFBAnalysis]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'CFBAnalysis', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL11.MSSQLSERVER\MSSQL\DATA\CFBAnalysis.mdf' , SIZE = 4096KB , MAXSIZE = UNLIMITED, FILEGROWTH = 1024KB )
 LOG ON 
( NAME = N'CFBAnalysis_log', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL11.MSSQLSERVER\MSSQL\DATA\CFBAnalysis_log.ldf' , SIZE = 1024KB , MAXSIZE = 2048GB , FILEGROWTH = 10%)
GO
ALTER DATABASE [CFBAnalysis] SET COMPATIBILITY_LEVEL = 110
GO
